{
  "name": "cairo-backend",
  "version": "1.0.0",
  "description": "Modern Node.js RESTful API backend with TypeScript",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.js",
    "build": "node esbuild.config.js --production",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "tsoa:spec": "tsoa spec",
    "tsoa:routes": "tsoa routes",
    "tsoa:generate": "npm run tsoa:spec && npm run tsoa:routes"
  },
  "keywords": [
    "express",
    "typescript",
    "rest-api",
    "backend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.14.2",
    "joi": "^17.13.3",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "express-rate-limit": "^7.4.0",
    "tsoa": "^6.4.0",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^22.5.4",
    "@types/swagger-ui-express": "^4.1.6",
    "typescript": "^5.6.2",
    "esbuild": "^0.23.1",
    "vitest": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "@vitest/coverage-v8": "^2.1.1",
    "tsx": "^4.19.1"
  }
}
